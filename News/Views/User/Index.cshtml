@using News.Models;
@model IEnumerable<UserModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts
{
    <script src="~/Scripts/app/myApp.js"></script>
}
<div>
    <h2>All users</h2>
    
    <table id="userTable">
        <thead>
        <tr>
            <td id="userId">ID</td>
            <td id="userName">First Name</td>
            <td id="userLastName">Last Name</td>
            <td id="userEmail">Email</td>
        </tr>
        </thead>
        <tbody id="users">

        </tbody>
    </table>
<div id="details">
</div>

</div>
